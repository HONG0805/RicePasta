<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.SpringWeb.RicePasta.Mapper.UserMapper">
<!-- 회원가입 -->
    <insert id="saveUser" parameterType="com.SpringWeb.RicePasta.VO.UserVO">
        INSERT INTO USER(USERID,USERPASSWORD,USERNAME,USERPHONENUM,USERAUTH,APPENDDATE,UPDATEDATE)
        VALUES(#{userID},#{userPassword},#{userName},#{userPhoneNum},#{userAuth},#{AppendDate},#{UpdateDate});
    </insert>
</mapper>
